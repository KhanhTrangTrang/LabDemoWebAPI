@{
    ViewData["Title"] = "Result";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="w-100 border border-dark p-3 rounded">
    <div class="mt-2 mb-2">
        <a type="button" class="btn btn-primary float-right" asp-controller="Home" asp-action="Logout" )">Log out</a>
    </div>
    <div class="d-flex flex-column justify-content-center">
        <div>@ViewBag.Result</div>
        <div class="mt-5">
            <a type="button" class="btn btn-primary w-25" asp-action="Index" )">OK</a>
        </div>
    </div>
</div>

@section Scripts
{
    @{
        <partial name="_ValidationScriptsPartial" />
    }
}
